{
    "name": "clickbar/laravel-magellan",
    "description": "This package provides functionality for working with the postgis extension.",
    "keywords": [
        "clickbar",
        "laravel",
        "postgis"
    ],
    "homepage": "https://github.com/clickbar/laravel-magellan",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian",
            "email": "ah@clickbar.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Clickbar\\Magellan\\": "src",
            "Clickbar\\Magellan\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clickbar\\Magellan\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clickbar\\Magellan\\MagellanServiceProvider"
            ],
            "aliases": {
                "Magellan": "Clickbar\\Magellan\\Facades\\Magellan"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
